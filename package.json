{
  "name": "devstree-chat-app",
  "version": "1.0.0",
  "description": "This is a web app which is used to chat ",
  "main": "index.js",
  "scripts": {
    "build": "cd server && npm && cd ../client && npm && npm run build",
    "server": "npm run dev --prefix server",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
